{
  "javascript.validate.enable": true,
  "editor.formatOnSave": true,
  "prettier.printWidth": 100,
  "prettier.singleQuote": true,
  "prettier.trailingComma": "all",
  "editor.tabSize": 2,
  "search.exclude": {
    "**/android": true,
    "**/ios": true
  },
  "git.ignoreLimitWarning": true
}
